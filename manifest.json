{
    "manifest_version": 3,
    "name": "YouTube 自動翻譯簡體",
    "version": "1.1",
    "description": "自動開啟自動翻譯把簡體轉成繁體",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "icons": {
      "128": "icon.png"
    },
    "content_scripts": [
      {
        "matches": ["*://www.youtube.com/*"],
        "js": ["content.js"]
      }
    ]
  }
  